<UserControl x:Class="FileViewer.PreViewControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:Lang="clr-namespace:XLY.SF.Framework.Language.PageLanguage;assembly=XLY.SF.Framework.Language"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <TabControl>
        <TabItem Header="{Binding XPath=FilePreviewLanguage/Preview,Source={x:Static Lang:PageLanguageHelper.XmlProvider}}" x:Name="Preview">            
        </TabItem>
        <TabItem Header="{Binding XPath=FilePreviewLanguage/HEX,Source={x:Static Lang:PageLanguageHelper.XmlProvider}}"  x:Name="HexPreview">           
        </TabItem>
    </TabControl>

</UserControl>
